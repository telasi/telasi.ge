<%= forma_for @file, title: t('models.network.change.send_new_file'), icon: '/icons/upload-cloud.png', collapsible: true, multipart: true do |f|
  f.file_field 'file', label: t('models.general.chose_file'), required: true
  f.submit t('models.general.actions.save')
  f.cancel_button network_new_customer_url(id: @application.id, tab: 'files')
end %>