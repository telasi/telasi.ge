<div class="row">
  <div class="col-sm-3"></div>
  <div class="col-sm-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="modal-title" id="myModalLabel">Printer inventory number</h4>
      </div>
      <%= form_tag stamba_asset_url, role: 'form', method: 'get' do %>
        <div class="panel-body">
          <%= text_field_tag "invno", params[:invno] %>
        </div>
        <div class="panel-footer">
          <%= submit_tag 'OK', class: ['btn', 'btn-primary'] %>
        </div>
      <% end %>
    </div>
  </div>
</div>